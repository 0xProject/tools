{
    "name": "@0x/utils",
    "version": "7.1.0",
    "engines": {
        "node": ">=6.12"
    },
    "description": "0x TS utils",
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "scripts": {
        "build": "tsc -b",
        "build:ci": "yarn build",
        "clean": "shx rm -rf lib",
        "watch": "tsc -b -w",
        "lint": "tslint --format stylish --project .",
        "fix": "tslint --fix --format stylish --project .",
        "test": "yarn run_mocha",
        "test:circleci": "yarn test:coverage",
        "run_mocha": "mocha --require source-map-support/register --require make-promises-safe 'lib/test/**/*_test.js' 'lib/test/*_test.js'  --bail --exit",
        "test:coverage": "nyc npm run test --all && yarn coverage:report:lcov",
        "coverage:report:lcov": "nyc report --reporter=text-lcov > coverage/lcov.info"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/tools.git"
    },
    "bugs": {
        "url": "https://github.com/0xProject/tools/issues"
    },
    "homepage": "https://github.com/0xProject/tools/tree/main/utils",
    "devDependencies": {
        "@0x/tslint-config": "^4.1.5",
        "@types/detect-node": "2.0.0",
        "@types/lodash": "4.14.104",
        "chai": "^4.0.1",
        "chai-as-promised": "^7.1.0",
        "chai-bignumber": "^3.0.0",
        "dirty-chai": "^2.0.1",
        "make-promises-safe": "^1.1.0",
        "mocha": "^6.2.0",
        "npm-run-all": "^4.1.2",
        "shx": "^0.2.2",
        "tslint": "5.11.0",
        "typescript": "4.2.2"
    },
    "dependencies": {
        "@0x/types": "^3.3.7",
        "@0x/typescript-typings": "^5.3.2",
        "@ethersproject/abi": "^5.0.0",
        "@types/mocha": "^5.2.7",
        "@types/node": "12.12.54",
        "abortcontroller-polyfill": "^1.1.9",
        "bignumber.js": "~9.0.2",
        "chalk": "^2.3.0",
        "detect-node": "2.0.3",
        "ethereum-types": "^3.7.1",
        "ethereumjs-util": "^7.1.5",
        "isomorphic-fetch": "^3.0.0",
        "js-sha3": "^0.7.0",
        "lodash": "^4.17.21"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "4f91bfd907996b2f4dd383778b50c479c2602b56"
}
